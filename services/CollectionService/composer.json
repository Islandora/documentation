{
    "name": "islandora/collection-service",
    "description": "RESTful service providing PCDM Collections in Fedora 4",
    "repositories": [{
                "type": "path",
                "url": "../ResourceService"
    },
    {
                "type": "path",
                "url": "../TransactionService"
    },
    {
                "type" : "path",
                "url" : "../../../chullo"
    }],
    "require": {
        "islandora/chullo": "dev-thinCollectionController",
        "islandora/resource-service" : "dev-updates-thinCollection",
        "islandora/transaction-service" : "dev-updates-thinCollection",
        "silex/silex": "^1.3",
        "symfony/config": "^3.0",
        "twig/twig": "^1.23",
        "symfony/yaml": "^3.0",
        "easyrdf/easyrdf": "^0.9.1",
        "ml/json-ld": "^1.0"
    },
    "autoload": {
        "psr-4": {"Islandora\\CollectionService\\": "src/"}
    },
    "authors": [
        {
            "name": "Diego Pino Navarro",
            "email": "dpino@krayon.cl"
        },
        {
